{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USERNAME};Password=${DB_PASSWORD}",
    "Redis": "${REDIS_HOST}:${REDIS_PORT}"
  },
  "Redis": {
    "CacheTimeout": "${REDIS_CACHE_TIMEOUT}"
  }
}
